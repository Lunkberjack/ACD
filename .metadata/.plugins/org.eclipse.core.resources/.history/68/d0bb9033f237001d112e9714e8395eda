package main;
import java.io.FileWriter;
import java.io.IOException;
/**
 * Pues eso
 * @author LuciaLM
 * @version 1.0
 */
public class LecturaEscritura {
	public static void main(String[] args) {
		try {
			escribir();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

	public static void escribir() throws IOException {
			FileWriter fw = new FileWriter("/home/lucia/Escritorio/prueba.txt");
			fw.write("8=========================================================================D");
			fw.close();
	}
}